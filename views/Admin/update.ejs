      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <div class="main-panel">
          <div class="content-wrapper pb-0">


            <!-- main content gose here  -->

            <div class="row" >

                <div class="col-12" style=" background-color: #f5f5f5; padding: 20px; margin: 20px; border-radius: 10px; display:flex; justify-content: center; align-items: center;">
                    
                  <form action="/form/update/<%=candidate.id%>" method="post" enctype="multipart/form-data">
                    <div class="form-row">
                      <div class="form-group col-md-6">
                        <label for="inputName">Full Name:</label>
                        <input type="text" class="form-control" id="inputName" placeholder="Full Name" name="Name" value="<%=candidate.Name%>">
                      </div>
                      <div class="form-group col-md-6">
                        <label for="inputUid">UID:</label>
                        <input type="text" class="form-control" id="inputUid" placeholder="UID0123456789" name="Uid" value="<%=candidate.Uid%>">
                      
                      </div>
                      <div class="form-group col">
                        <label for="inputDesignation">Designation:</label>
                        <input type="text" class="form-control" id="inputDesignation" placeholder="Designation" name="Designation" value="<%=candidate.Designation%>">
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="inputWork">Work:</label>
                      <input type="text" class="form-control" id="inputWork" placeholder="Work Type" name="Work" value="<%=candidate.Work%>">
                    </div>
                    <div class="form-group">
                      <label for="inputArea">Area:</label>
                      <input type="text" class="form-control" id="inputArea" placeholder="Apartment, studio, or floor" name="Area" value="<%=candidate.Area%>">
                    </div>
                    <div class="form-row">
                      <div class="form-group col-md-4">
                        <label for="inputValidityStart"> Strat Validity : ><%= candidate.ValidityStart %></label>
                        <input type="date" class="form-control" id="inputValidityStart" placeholder="Start Validity" name="ValidityStart" required>
                        
                      </div>
                      <div class="form-group col-md-4">
                        <label for="inputValidityEnd">End Validity:><%= candidate.ValidityEnd %></label>
                        <input type="date" class="form-control" id="inputValidityEnd" placeholder="End Validity" name="ValidityEnd" required>
                        
                      </div>
                      <div class="form-group col-md-4">
                        <label for="inputPhoto">Photo:</label>
                        <input type="file" class="form-control" id="inputPhoto" data-toggle="tooltip" data-placement="right" title="Photo Must be 200 KB Or Less" name="photo">
                        <img src="/<%=candidate.photo%>" alt="Candidate Photo" class="col-md-12">
                      </div>
                    </div>
                   
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
                </div>
            </div>

            <!-- end contant  -->
            
          </div>
          <!-- content-wrapper ends -->
          <!-- partial:partials/_footer.html -->
