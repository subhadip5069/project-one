    <div class="container mt-5">
  <h1 class="text-center">SCEARCH CANDIDATE BY UID <hr></h1>
  <form action="/user/scearch" method="GET" class="mb-4">
    <div class="mb-3">
      <label for="uid" class="form-label">Enter UID</label>
      <input type="text" id="uid" name="uid" class="form-control" required>
    </div>
    <button type="submit" class="btn btn-primary">Search</button>
  </form>

  <% if (!candidate) { %>
    <div class="alert alert-warning" role="alert">
      No candidate found. Please check the UID and try again.
    </div>
  <% } else { %>
    <div class="card shadow-lg mb-3" style="max-width: 540px; margin: 0 auto;">
      <div class="card-header bg-primary text-white">Candidate Profile</div>
      <div class="card-body">
        <blockquote class="blockquote mb-0">
          <img src="/<%= candidate.photo %>" class="img-fluid rounded-start mb-3" alt="Candidate Photo">
          <p><strong>Name:</strong> <%= candidate.Name %></p>
          <p><strong>UID:</strong> <%= candidate.Uid %></p>
          <p><strong>Designation:</strong> <%= candidate.Designation %></p>
          <p><strong>Work:</strong> <%= candidate.Work %></p>
          <p><strong>Area:</strong> <%= candidate.Area %></p>
          <p><strong>Validity:</strong> <%= candidate.ValidityStart %> to <%= candidate.ValidityEnd %></p>
          
          <% if (new Date(candidate.ValidityEnd) < new Date()) { %>
            <div class="alert alert-danger mt-3" role="alert">
              Validity expired on <%= candidate.ValidityEnd %>.
            </div>
          <% } %>
          
          <% if (candidate.Email) { %>
            <p><strong>Email:</strong> <%= candidate.Email %></p>
          <% } %>
          <footer class="blockquote-footer">Profile last updated on <cite title="Timestamp"><%= candidate.updatedAt %></cite></footer>
        </blockquote>
      </div>
    </div>
  <% } %>
</div>

        

